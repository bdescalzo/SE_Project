<AnchorPane fx:id="chatPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eus.ehu.ui_mockup.presentation.ProjectController">
   <children>
      <TabPane fx:id="mainBar" prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" tabMinWidth="179.0">
        <tabs>
          <Tab fx:id="chatTab" text="Chat">
